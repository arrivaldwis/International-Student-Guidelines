<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="#ececec"
    tools:context="com.sandywinata.isg.Undergraduate">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <TextView
            android:id="@+id/txtHeaderUndergraduate"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="UNDERGRADUATE PROGRAMS"
            android:textAlignment="center"
            android:textStyle="bold"/>
        <TextView
            android:layout_below="@id/txtHeaderUndergraduate"
            android:id="@+id/txtdetailUndergraduate"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:text="LIST OF PROGRAMS"/>
    </RelativeLayout>
</LinearLayout>
